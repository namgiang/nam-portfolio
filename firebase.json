{
  "hosting": {
    "public": "build/default",
    "redirects": [
      {
        "source": "/",
        "destination": "/home",
        "type": 301
      }
    ],
    "rewrites": [
      {
        "source": "!{/bower_components,/src}/**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      { "headers": [{"key": "Cache-Control", "value": "no-cache"}] }
    ]
  }
}
